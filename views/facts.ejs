<body id="body">
   <div class="container">
    <div class="row">
<%- include('partials/leftBar') %>
<%- include('partials/navbar') %>
<div class="col-lg-10 second-col" >
    <h1>Interesting Weather Facts List:</h1>
   
     
   
    <div class="facts">
        <ul>
                <% var num = Math.floor((Math.random() * 5) + 1) %>
        <%  for(var i=num;i<num+7;i++){ %>
            <li><%= facts[i] %></li>
        <%  } %>
        </ul>
        
       
      </div>
    </div>
</div>
